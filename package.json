    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "jszip": "^3.10.1",
    "postprocessing": "^6.38.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^7.10.1",
    "three": "^0.160.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.0"
  }
}
