{
  "name": "dev-cracks",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"WEB,BACKOFFICE\" -c \"blue,green\" \"npm:dev:web\" \"npm:dev:backoffice\"",
    "dev:web": "cd web && npm run dev",
    "dev:backoffice": "cd backoffice && npm run dev",
    "build": "npm run build:web && npm run build:backoffice",
    "build:web": "cd web && npm run build",
    "build:backoffice": "cd backoffice && npm run build",
    "install:all": "npm install && cd web && npm install && cd ../backoffice && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
