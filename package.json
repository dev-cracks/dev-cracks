{
  "name": "dev-cracks",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:main": "vite",
    "dev:web": "npm --prefix web run dev",
    "dev:backoffice": "npm --prefix backoffice run dev",
    "build": "npm run install:all && npm run build:web && npm run build:backoffice",
    "build:web": "npm --prefix web run build",
    "build:backoffice": "npm --prefix backoffice run build",
    "install:all": "npm install && npm --prefix web install && npm --prefix backoffice install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite": "^5.0.0",
    "express": "^4.18.2"
  }
}
